<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude camera data from cloud backup -->
        <exclude domain="sharedpref" path="camera_settings.xml"/>
        <exclude domain="database" path="opencv_cache.db"/>
    </cloud-backup>
    <device-transfer>
        <!-- Allow device transfer of app data -->
        <include domain="sharedpref" path="."/>
        <include domain="database" path="."/>
    </device-transfer>
</data-extraction-rules>
